{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "necolas/normalize.css",
                "version": "3.0.1",
                "dist": {
                    "type": "file",
                    "url": "https://raw.githubusercontent.com/necolas/normalize.css/3.0.1/normalize.css"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",

        "components/jquery": "1.11.1",
        "components/jqueryui": "1.11.1",
        "necolas/normalize.css": "3.0.1",
        "components/bootstrap": "3.2.0",
        "fortawesome/font-awesome": "v4.1.0",
        "braincrafted/bootstrap-bundle": "~2.1",
        "appsco/component-share": "~1.0",
        "doctrine/doctrine-migrations-bundle": "dev-master#f7138381aa884c0f679da4de41e369b94ead9cd3",
        "doctrine/migrations": "dev-master#46a031ddaea47d0685200027cfe8c83b02aee6f6",
        "friendsofsymfony/rest-bundle": "~1.4",
        "jms/serializer-bundle": "0.13.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "BWC\\Share\\Composer\\Script::vendor2web"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "BWC\\Share\\Composer\\Script::vendor2web"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin",
        "component-dir": "web/components"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        },
        "vendor2web": {
            "necolas/normalize.css": "necolas_normalize.css",
            "fortawesome/font-awesome": "fortawesome_font-awesome"
        }
    }
}
